def deltaR( eta1, phi1, eta2, phi2):
    deta = eta1 - eta2
    dphi = phi1 - phi2
    if dphi >= math.pi: dphi -= 2*math.pi
    elif dphi < -math.pi: dphi += 2*math.pi
    return math.sqrt(deta*deta + dphi*dphi)

